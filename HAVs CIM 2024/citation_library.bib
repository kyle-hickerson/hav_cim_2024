@article{brownRashImpulsivityReward2021,
  title = {Rash Impulsivity, Reward Seeking and Fear of Missing out as Predictors of Texting While Driving: {{Indirect}} Effects via Mobile Phone Involvement},
  author = {Brown, Patricia M. and George, Amanda M. and Rickwood, Debra J.},
  year = {2021},
  month = mar,
  journal = {Personality and Individual Differences},
  volume = {171},
  pages = {110492},
  issn = {0191-8869},
  doi = {10.1016/j.paid.2020.110492},
  abstract = {Young drivers exhibit high levels of risky driving behaviour, including texting while driving (TWD). The aim of this study was to examine the influence of personality (rash impulsivity, reward seeking), fear of missing out (FOMO) and mobile phone involvement (MPI) on frequency of TWD. Six hundred and twelve young drivers aged 17 to 24~years completed an online survey including these measures, and frequency of sending and reading TWD in the prior week. Rash impulsivity and reward seeking, as well as MPI, predicted both modes of TWD, while FOMO only predicted sending. In addition, rash impulsivity, reward seeking and FOMO all had significant indirect effects on sending and reading TWD via MPI. Findings highlight the importance of considering indirect relationships of personality via MPI on phone use while driving.},
  keywords = {Cell phone,FOMO,Impulsivity,Mobile phone,Mobile phone involvement,Personality,Texting,Young driver}
}

@article{brownSimulatorSicknessQuestionnaire2022,
  title = {The Simulator Sickness Questionnaire, and the Erroneous Zero Baseline Assumption},
