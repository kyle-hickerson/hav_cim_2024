@article{colettaCivilianMonitoringMilitary,
  title = {Civilian {{Monitoring}} of {{U}}.{{S}}. {{Military Operations}} in the {{Information Age}}},
  author = {Coletta, Damon and Feaver, Peter D.},
  journal = {Armed Forces \& Society},
  volume = {33},
  number = {1},
  pages = {106--126},
  doi = {10.1177/0095327X05282530},
  abstract = {Recent research on U.S. civil-military relations has applied principal-agent logic to analyze the post-cold war friction between civilian authorities and top military commanders. This article proposes a greater emphasis on bargaining to focus on the effects of new monitoring technologies available to the civilian principal in the information age. As monitoring capabilities increase and military agents perceive their autonomy disappearing, tacit bargaining over the president's level of resource commitment to a crisis should become more prevalent. This idea receives support from a comparison across case studies of the limited use of force taken from different technological eras. A new style of civil-military bargaining presents both challenges and opportunities to the traditional conception of military professionalism.}
}

