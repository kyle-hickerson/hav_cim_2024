@article{et.al.ImmersedGritProbing2021,
  title = {Immersed with {{Grit}}: {{Probing}} the {{Mindset}} of {{Future Hospitality Professionals}}},
  author = {{Et. al.}, Ashika Miriam Tharakan},
  year = {2021},
  month = apr,
  journal = {Turkish Journal of Computer and Mathematics Education (TURCOMAT)},
