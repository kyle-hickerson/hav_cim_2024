@phdthesis{malawskiCyberGamificationUse2018,
  type = {M.{{S}}.},
  title = {Cyber {{Gamification}}: {{The Use}} of {{Capture}} the {{Flag}} ({{CTF}}) and {{Similar Exercises}} to {{Develop Critical-Thinking}} and {{Problem-Solving Skills}} in {{Cyber Graduates}}},
  author = {Malawski, Brian},
  year = {2018},
  journal = {ProQuest Dissertations and Theses},
  address = {United States -- New York},
  abstract = {With data breaches becoming the norm instead of the exception, there is need to develop new strategies to counter them. In addition, employers are demanding cyber-security professionals with the skill and expertise to combat these rising threats. A fairly new concept called cyber gamification has developed to train that new talent to take them on, however, it has not become a main program within most universities. Therefore, the purpose of this study was to understand the reasoning as to why higher educational institutions lack the cyber gamification concept in their curricula. After an extensive review, there are many factors influencing why higher educational institutions lack an in demand cyber gamification program. Some of those factors include lack of financial resources, the high educational learning time investment and the negative perception of the program as a whole. This study concluded that cyber gamification teaches students highly sought out critical thinking skills as well as enforcing theoretical concepts. With minor adjustments to the mentioned barriers of inclusion, it was discovered that any higher educational institution can implement a cyber gamification program.},
  collaborator = {DeCarlo, Robert},
  isbn = {978-0-355-90844-2},
  langid = {english},
  school = {Utica College},
  keywords = {0443:Educational evaluation,Education,Educational evaluation},
  annotation = {10812799}
}

@article{maModeratingRoleSocial2020,
  title = {The {{Moderating Role}} of {{Social Identity}} and {{Grit}} in the {{Association Between Parental Control}} and {{School Adjustment}} in {{Chinese Middle School Students}}},
  author = {Ma, Chunhua and Ma, Yongfeng and Lan, Xiaoyu},
  year = {2020},
  month = apr,
  journal = {Frontiers in Psychology},
  volume = {11},
  publisher = {Frontiers Media SA},
  doi = {10.3389/fpsyg.2020.00677},
  abstract = {Although the proliferation of empirical research has documented the association between parental control and school adjustment, findings of this linkage are still inconclusive. Moreover, fewer efforts have been made to address this association in middle school students. Guided by an ecological framework, the current study aimed to integrate the conflicting findings into a coherent body of knowledge, paying particular attention to two research purposes: (a) to examine the association between parental control and three objective indicators of school adjustment (social competence, academic grades, and peer acceptance) and (b) to explore whether this association was moderated by individual characteristics of social identity and grit. A total of 120 Chinese middle school students (42.5\% females) aged between 13 and 15 years old were recruited for this study, and research data were gathered from multiple sources. To be specific, students were asked to complete a set of self-report questionnaires concerning parental control, social identity, and grit. Meanwhile, school-related social competence was rated by head teachers; academic grades were obtained from school records; and peer acceptance was assessed by sociometric nominations. The results from hierarchical regression analyses showed that parental control was negatively associated with academic grades. Moreover, when reporting higher levels of social identity, parental control was negatively related to social competence and peer acceptance for those students with lower levels of grit. Our findings suggest that parental control can dampen middle school students' academic performance, and low levels of grit can magnify the detrimental effect of parental control on social competence and peer acceptance in middle school students who regard themselves as closely connected to social groups.}
}

