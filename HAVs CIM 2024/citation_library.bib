@article{postDeepLevelTeamComposition2012,
  title = {Deep-{{Level Team Composition}} and {{Innovation}}: {{The Mediating Roles}} of {{Psychological Safety}} and {{Cooperative Learning}}},
  author = {Post, Corinne},
  year = {2012},
  month = oct,
  journal = {Group \& Organization Management},
  volume = {37},
  number = {5},
  pages = {555--588},
  publisher = {SAGE Publications Inc},
  issn = {1059-6011},
  doi = {10.1177/1059601112456289},
  urldate = {2022-12-11},
  abstract = {This study proposes and tests a process model linking deep-level team composition and innovation. Two cognitive styles, sequential thinking and connective thinking, are the deep-level team attributes hypothesized to affect team innovation. Psychological safety and cooperative learning are hypothesized to link cognitive styles and innovation at the team level. Findings from a sample of 83 natural innovation teams with dual-source data indicate that sequential thinking contributes to decreases in team innovation by inhibiting psychological safety, while connective thinking helps improve team innovation through increased cooperative learning. Implications for research and practice are discussed.}
}

@article{postigoTemporalStabilityGrit2020,
  title = {Temporal {{Stability}} of {{Grit}} and {{School Performance}} in {{Adolescents}}: {{A Longitudinal Perspective}}},
  author = {Postigo, {\textbackslash}' Alvaro and Cuesta, Marcelino and en {Fern{\textbackslash}' andez-Alonso}, Rub{\textbackslash}' and {Garc{\textbackslash}' ia-Cueto}, Eduardo and e {Mu{$\sim$} niz}, Jos{\textbackslash}'},
  year = {2020},
  month = dec,
  journal = {Psicolog{\textbackslash}' ia Educativa},
  volume = {27},
  number = {1},
  pages = {77--84},
  publisher = {Colegio Oficial de Psicologos de Madrid},
  doi = {10.5093/psed2021a4},
  abstract = {Grit is a construct that is related to perseverance and passion for achieving set goals. Its relationship to school performance has been widely researched although the results are not conclusive. The aim of this study was to examine the temporal stability of grit and its relationship to adolescents' school performance. A sample of 5,371 students were evaluated at two time points, four years apart. At first evaluation, mean age was 9.9 years old (SD = 0.41), at the second it was 13.87 (SD = 0.82). A longitudinal design was used, and the data were analysed using analysis of variance, factor analysis, and structural equations models. Adolescents' academic grit and school achievement fall between ages of 10 and 14. Adolescents with higher levels of grit had higher academic achievement. The two constructs are correlated, which is why interventions aimed at improving academic grit could be important in improving school performance.}
}

